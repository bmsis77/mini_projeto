desempenho <- read.delim("C:/Users/Aluno/Downloads/desempenho.txt")
View(desempenho)

head(desempenho)
tail(desempenho)

mean(desempenho$matematica)
quantile(desempenho$matematica, 0.25)

table(desempenho$genero)

hist(
  desempenho$literatura,
  main = "Distribuição das Pontuações em Literatura",
  xlab = "Pontuação em Literatura",
  ylab = "Frequência de Alunos",
  col = "lightblue",
  border = "black"
)

boxplot(
  desempenho$matematica,
  main = "Boxplot das Pontuações em Matemática",
  ylab = "Pontuação"
)
